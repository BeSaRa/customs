{
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "singleQuote": true
      }
    },
    {
      "files": "*.html",
      "options": {
        "parser": "angular",
        "printWidth": 100,
        "bracketSameLine": true,
        "singleAttributePerLine": true
      }
    }
  ]
}
